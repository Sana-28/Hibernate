package com.bridgeit.HibernateLogin;

import java.util.Scanner;

import org.hibernate.tool.schema.internal.exec.ScriptSourceInputNonExistentImpl;

import com.bridgeit.HibernateLogin.dao.UserDAO;
import com.bridgeit.HibernateLogin.dto.User;

/**
 * Hello world!
 *
 */
public class App {
	static Scanner scanner = new Scanner(System.in);

	public static void main(String[] args) {

		System.out.println("Enter Number of Object:");
		UserDAO userDao = new UserDAO();

		int n = scanner.nextInt();
		for (int i = 0; i < n; i++) {

			userDao.saveUser(UserDAO.createUser());

		}
		/*
		 * UserDAO userDao = new UserDAO();
		 * 
		 * userDao.saveUser(user); // userDao.saveUser(user1);
		 * System.out.println("Object saved successfully..");
		 * 
		 * For deleting User deleteUser = new User(); UserDAO userDAO = new
		 * UserDAO();
		 * 
		 * userDAO.deleteUser(user);
		 * System.out.println("Object deleted successfully...");
		 */

	}
}
